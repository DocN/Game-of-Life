        DoubleLL<Integer> test = new DoubleLL<Integer>(5, null, null);
        test.addToEnd(6);
        test.addToFront(1);
        test.removeFromEnd();
        test.printList();
        System.out.println(test.getFirst().getData());
        System.out.println(test.getLast().getData());
