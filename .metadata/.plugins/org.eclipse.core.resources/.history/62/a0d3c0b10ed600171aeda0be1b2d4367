         * DoubleLL<Integer>(1, null, null); // magic numbers used in testing
         * try { test.addToFront(2); test.addToFront(3); test.addToFront(5); }
         * catch (CouldNotAddException e) { System.out.println(e.getMessage());
         * }
         * 
         * // test.printList(); try { test.removeFromFront(); } catch
